{
  "name": "@therealklanni/eslint-config",
  "version": "1.0.0-beta.6+2ba2447",
  "description": "Shareable ESLint config",
  "license": "MIT",
  "author": "Kevin Lanni <therealklanni@gmail.com> (https://github.com/therealklanni)",
  "homepage": "https://github.com/therealklanni/eslint/tree/master/packages/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/therealklanni/eslint/issues"
  },
  "keywords": [
    "eslintconfig",
    "therealklanni"
  ],
  "main": "lib/index.js",
  "scripts": {
    "@deps": "/usr/bin/env bash ./scripts/dev-deps.sh",
    "lint": "eslint .",
    "postpublish": "rm -rf dist",
    "prepack": "mkdir -p dist; cp lib/* dist; cp ../../LICENSE.txt README.md package.json dist"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "eslintConfig": {
    "extends": [
      "./lib/prettier",
      "./lib/node"
    ]
  },
  "eslintIgnore": [
    "test.js"
  ],
  "gitHead": "2ba2447c2b2d7d35c6c86281a840f723a0a218b5",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therealklanni/eslint.git"
  },
  "peerDependencies": {
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.4"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.3.0"
  }
}
