{
  "name": "@therealklanni/eslint-config",
  "version": "1.0.0-beta.14",
  "description": "Shareable ESLint config",
  "license": "MIT",
  "author": "Kevin Lanni <therealklanni@gmail.com> (https://github.com/therealklanni)",
  "homepage": "https://github.com/therealklanni/eslint/tree/master/packages/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/therealklanni/eslint/issues"
  },
  "keywords": [
    "eslintconfig",
    "therealklanni"
  ],
  "main": "index.js",
  "scripts": {
    "@deps": "/usr/bin/env bash ./scripts/dev-deps.sh",
    "lint": "eslint .",
    "postpublish": "rm -rf dist",
    "prepack": "mkdir -p dist; cp ../../LICENSE.txt README.md package.json *.js dist"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "eslintConfig": {
    "extends": [
      "./prettier",
      "./node"
    ]
  },
  "eslintIgnore": [
    "test.js"
  ],
  "files": [
    "*"
  ],
  "gitHead": "8b11f98ef107e6e2ef740e3adab31b6777ce4e3d",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therealklanni/eslint.git"
  },
  "peerDependencies": {
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.4"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.3.0"
  }
}
